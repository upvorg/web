{
    "name": "Your Anime",
    "short_name": "Your Anime",
    "description": "Anime Web App.\nMore info & source code here: https://github.com/upvorg/cdn",
    "version": "0.0.1",
    "manifest_version": 2,
    "app": {
        "background": {
          "scripts": ["js/background.js"]
        }
      },
    "permissions": [
        "notifications",
        "webview",
        {"fileSystem": ["write"]},
        "storage",
        "unlimitedStorage",
        "fullscreen"
      ],
    "icons": {
        "16": "ic_launcher_round.png",
        "32": "ic_launcher_round.png",
        "60": "ic_launcher_round.png",
        "64": "ic_launcher_round.png",
        "90": "ic_launcher_round.png",
        "128": "ic_launcher_round.png"
      },
    "theme_color": "#6668ab",
    "background_color": "#6668ab",
    "display": "standalone"
}